{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**/*.yml" ],
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": [
      {
        "files": [ "overwrite/**.md" ],
        "exclude": [ "obj/**", "_site/**" ]
      }
    ],
    "dest": "generated-site-content",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "statictoc" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "globalMetadata": {
      "_appTitle": "NetTopologySuite.IO.GPX",
      "_enableSearch": true,
      "_gitContribute": {
        "apiSpecFolder": "doc/overwrite"
      }
    }
  }
}